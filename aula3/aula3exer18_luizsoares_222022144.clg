algoritmo SomatorioDados;
// Síntese
//  Objetivo:  
//  Entrada :
//  Saída   :


principal
ALGORITMO 
real dados[70];
real soma;
inteiro contador;

soma = 0;

PARA i DE 1 ATÉ 70 FAÇA
    ESCREVA "Digite o valor ", i, ": "
    LEIA dados[i]
    soma <- soma + dados[i]
FIM PARA

ESCREVA "O somatório dos valores é: ", soma

FIM ALGORITMO

fimPrincipal

funcao inteiro (dados[])
